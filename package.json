{
    "name": "node-yolog",
    "description": "Logging module for nodejs.",
    "version": "2.0.0",
    "license": "MIT",
    "esnext": {
        "main": "src/Yolog.js",
        "browser": "src/web/index.js"
    },
    "author": {
        "name": "Johannes Tegn√©r",
        "email": "jitedev@gmail.com"
    },
    "keywords": [
        "logging",
        "formatted",
        "simple",
        "toggleable",
        "jitesoft",
        "krig"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Johannestegner/node-yolog"
    },
    "eslintConfig": {
        "parser": "babel-eslint",
        "extends": "semistandard",
        "env": {
            "shared-node-browser": true,
            "es6": true,
            "commonjs": true
        },
        "overrides": [
            {
                "files": [
                    "*.node.js"
                ],
                "env": {
                    "node": true,
                    "browser": false
                }
            },
            {
                "files": [
                    "*.web.js"
                ],
                "env": {
                    "browser": true,
                    "node": false
                }
            },
            {
                "files": [
                    "*.test.js"
                ],
                "env": {
                    "jest": true
                }
            }
        ]
    },
    "main": "dist/index.js",
    "readmeFilename": "README.md",
    "dependencies": {
        "@babel/runtime-corejs3": "^7.4.0"
    },
    "devDependencies": {
        "@babel/core": "^7.4.0",
        "@babel/plugin-proposal-class-properties": "^7.4.0",
        "@babel/plugin-proposal-decorators": "^7.4.0",
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.2.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
        "@babel/plugin-proposal-optional-chaining": "^7.2.0",
        "@babel/plugin-proposal-private-methods": "^7.4.0",
        "@babel/plugin-proposal-throw-expressions": "^7.2.0",
        "@babel/plugin-transform-runtime": "^7.4.0",
        "@babel/polyfill": "^7.4.0",
        "@babel/preset-env": "^7.4.1",
        "@jitesoft/events": "^1.1.3",
        "babel-eslint": "^10.0.1",
        "babel-jest": "^24.5.0",
        "babel-loader": "^8.0.5",
        "clean-webpack-plugin": "^2.0.1",
        "core-js": "^3.0.0",
        "cross-env": "^5.2.0",
        "eslint": "^5.15.3",
        "jest": "^24.5.0",
        "semistandard": "^13.0.1",
        "uglifyjs-webpack-plugin": "^2.1.2",
        "webpack": "^4.29.6",
        "webpack-cli": "^3.3.0"
    },
    "scripts": {
        "test": "semistandard /src && jest",
        "build:prod": "cross-env NODE_ENV=production && webpack -p",
        "build:dev": "cross-env NODE_ENV=develop && webpack"
    }
}
